{
    "name": "Rashmitagangani/db-switcher",
    "description": "A Laravel package for switching database connections easily",
    "type": "library",
    "require": {
        "php": "^8.2",
        "illuminate/support": "^8.0|^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Rashmitagangani\\DatabaseSwitcher\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rashmitagangani\\DatabaseSwitcher\\Providers\\DatabaseSwitcherServiceProvider"
            ],
            "aliases": {
                "DatabaseSwitcher": "Rashmitagangani\\DatabaseSwitcher\\Facades\\DatabaseSwitcher"
            }
        }
    }
}
